@{
    ViewData["Title"] = "Car Tracker";
}
@model List<Status>

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <table class="table table-striped table-bordered">
        <thead>
        <td>ID</td>
        <td>VIN</td>
        <td>Warranty Validity</td>
        <td>Latitude</td>
        <td>Longitude</td>
        </thead>
        <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>@obj.Id</td>
                <td>@obj.Vin</td>
                <td>@obj.WarrantyStatus</td>
                <td>@obj.Latitude</td>
                <td>@obj.Longitude</td>
            </tr>
        }
        </tbody>
    </table>
    
