CREATE TABLE inventory.Status (
	Id INTEGER UNSIGNED auto_increment NOT NULL,
	Vin varchar(100) NOT NULL,
	WarrantyStatus BOOL DEFAULT TRUE NOT NULL,
	Latitude DOUBLE DEFAULT 33.942791 NOT NULL,
	Longitude DOUBLE DEFAULT -118.410042 NOT NULL,
	CONSTRAINT Status_Id_PK PRIMARY KEY (Id),
	CONSTRAINT Status_Car_FK FOREIGN KEY (Vin) REFERENCES inventory.Car(Vin)
)
ENGINE=InnoDB
DEFAULT CHARSET=utf8mb4
COLLATE=utf8mb4_0900_ai_ci;

